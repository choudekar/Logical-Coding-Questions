package General;

public class Quiz4 {

	public static void main(String[] args) {
		String str= "JavaQuiz";
		
		System.out.println(str.substring(1, 4));
		//this line prints a substring of the str variable.
		//the substring method is used to extract a portion of the original string ,in this case
		//it starts at index 1(inclusive )
		//and ends at index 4 (exclusive)
	}
}
